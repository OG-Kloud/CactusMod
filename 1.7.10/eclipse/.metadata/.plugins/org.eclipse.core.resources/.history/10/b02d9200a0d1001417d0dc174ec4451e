public class Common {
	
	public static void register() {
		
		EntityRegistry.registerModEntity(EntityCactusTNT.class, "cactusTNT", EntityRegistry.findGlobalUniqueEntityId(), Cactus.class, 0, 10, false);
	}
	
	public static void registerRenderer() {
		
	}

}
