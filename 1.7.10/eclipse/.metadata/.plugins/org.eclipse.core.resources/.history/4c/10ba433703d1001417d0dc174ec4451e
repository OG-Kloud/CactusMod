package net.kloudspace.cactus.block;

import cpw.mods.fml.relauncher.Side;
import cpw.mods.fml.relauncher.SideOnly;
import net.kloudspace.cactus.block.blocks.KBlocks;
import net.kloudspace.cactus.utils.Reference;
import net.minecraft.block.BlockTNT;
import net.minecraft.util.IIcon;

public class KCactusTNT extends BlockTNT {
	
    @SideOnly(Side.CLIENT)
    public IIcon getIcon(int p_149691_1_, int p_149691_2_)
    {
        return p_149691_1_ == 0 ? KBlocks.cactusBrick.getBlockTextureFromSide(p_149691_1_) : p_149691_1_ == 0 ? KBlocks.cactusBrick.getBlockTextureFromSide(p_149691_1_) : Reference.MODID+":"+"cactustnt";
    }

}
